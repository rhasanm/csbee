    "version": "0.2.0",
    "configurations": [
        {
            "name": "(OSX) Launch",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/target/debug/${workspaceRootFolderName}",
            "args": ["/home/laboratory/datasets/banglalink/March_April.csv"],
            "cwd": "${workspaceRoot}",
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'csvew'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=csvew",
                    "--package=csvew",
                ],
                "filter": {
                    "name": "csvew",
                    "kind": "bin"
                }
            },
            "args": [
                "/home/laboratory/datasets/banglalink/March_April.csv"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'csvew'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=csvew",
                    "--package=csvew"
                ],
                "filter": {
                    "name": "csvew",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}